#
# red owns cube
#
set cube use off
!import -window "0x240001d" bla.png
!convert -crop 48x32+216+42 bla.png  images/b-ct.png
set cube use on
set cube owner 0
#
set cube value 1
!import -window "0x240001d" bla.png
!convert -crop 48x32+216+42 bla.png  images/b-ct-1.png
#
set cube value 2
!import -window "0x240001d" bla.png
!convert -crop 48x32+216+42 bla.png  images/b-ct-2.png
#
set cube value 4
!import -window "0x240001d" bla.png
!convert -crop 48x32+216+42 bla.png  images/b-ct-4.png
