distcheck-hook:
	chmod -R a+w $(distdir)
	-cp $(srcdir)/gnubg.weights $(distdir)/

br1.c:	makebearoff makebearoff1
	if [ ! -f br1.c ]; then \
	   ./makebearoff -o 6 -s 7999999 | ./makebearoff1 -o br1.c; \
	fi

