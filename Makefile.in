	$(AWK) -f $(srcdir)/copying.awk < $(srcdir)/COPYING > $@

# Create symlinks to all the necessary parts of $(srcdir), so that gnubg
# can run happily in the build directory.
all-local:
	@for i in gnubg_os0.bd gnubg_ts0.bd gnubg.gtkrc gnubg.scm \
	gnubg.weights; do \
	[ -s $$i ] || if [ -s $(srcdir)/$$i ]; then $(LN_S) $(srcdir)/$$i .; \
	fi; done
	@for i in gnubg.xml annealing.png rulfig1.png rulfig2.png rulfig3.png \
	rulfig4.png rulfig5.png texinfo.dtd; do [ -s $$i ] || \
	if [ -s $(srcdir)/doc/$$i ]; then $(LN_S) $(srcdir)/doc/$$i .; fi; done

gnubg_ts0.bd:
	@echo ' ** NOTE: For full strength in bearoff moves, GNU'
	@echo ' ** Backgammon requires a "gnubg_ts0.bd" file containing a'
	@echo ' ** database of endgame positions.  See the README'
	@echo ' ** file for details.'

gnubg.weights:
	@echo ' ** NOTE: For proper operation, GNU Backgammon requires'
	@echo ' ** a "gnubg.weights" file containing neural net'
	@echo ' ** parameters.  See the README file for details.'

@CROSS_COMPILING_TRUE@gnubg.wd: gnubg.weights makeweights
@CROSS_COMPILING_TRUE@	@echo ' ** NOTE: Since you are cross-compiling GNU Backgammon,'
@CROSS_COMPILING_TRUE@	@echo ' ** it is not possible to generate the gnubg.wd file'
@CROSS_COMPILING_TRUE@	@echo ' ** on the build system.  To create this file manually,'
@CROSS_COMPILING_TRUE@	@echo ' ** use a command like:'
@CROSS_COMPILING_TRUE@	@echo ' **   makeweights < gnubg.weights > gnubg.wd'
@CROSS_COMPILING_TRUE@	@echo ' ** on the host system.'
@CROSS_COMPILING_FALSE@gnubg.wd: gnubg.weights makeweights
@CROSS_COMPILING_FALSE@	-./makeweights < $< > $@

@CROSS_COMPILING_TRUE@gnubg_os0.bd:
@CROSS_COMPILING_TRUE@	@echo ' ** NOTE: Since you are cross-compiling GNU Backgammon,'
@CROSS_COMPILING_TRUE@	@echo ' ** it is not possible to generate the bearoff database'
@CROSS_COMPILING_TRUE@	@echo ' ** on the build system.  To create this file manually,'
@CROSS_COMPILING_TRUE@	@echo ' ** use a command like:'
@CROSS_COMPILING_TRUE@	@echo ' **   makebearoff -o 6 -s 7999999 > gnubg_os0.bd'
@CROSS_COMPILING_TRUE@	@echo ' ** on the host system.'

@CROSS_COMPILING_TRUE@bearoff-database: makebearoff
@CROSS_COMPILING_TRUE@	@echo ' ** NOTE: Since you are cross-compiling GNU Backgammon,'
@CROSS_COMPILING_TRUE@	@echo ' ** it is not possible to generate the bearoff database'
@CROSS_COMPILING_TRUE@	@echo ' ** on the build system.  To create this file manually,'
@CROSS_COMPILING_TRUE@	@echo ' ** use a command like:'
@CROSS_COMPILING_TRUE@	@echo ' **   makebearoff -t 6x6 > gnubg_ts0.bd'
@CROSS_COMPILING_TRUE@	@echo ' ** on the host system.'
@CROSS_COMPILING_FALSE@gnubg_os0.bd: makebearoff
@CROSS_COMPILING_FALSE@	[ -s $(srcdir)/gnubg_os0.bd ] || \
@CROSS_COMPILING_FALSE@	./makebearoff -o 6 -s 7999999 -f $(srcdir)/gnubg_os0.bd

@CROSS_COMPILING_FALSE@bearoff-database: makebearoff
@CROSS_COMPILING_FALSE@	@echo ' ** NOTE: you can generate huge one sided and two sided'
@CROSS_COMPILING_FALSE@	@echo ' ** bearoff databases for usage with gnubg'
@CROSS_COMPILING_FALSE@	@echo ' ** See README for more details'
@CROSS_COMPILING_FALSE@	./makebearoff -t 6x6 > $(srcdir)/gnubg_ts0.bd; $(MAKE) gnubg.bd

.PHONY: bearoff-database

